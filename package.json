{
  "name": "heimruf-deck-builder",
  "version": "1.7.16",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "deploy": "firebase deploy",
    "lint": "ng lint && prettier . --check",
    "fix": "ng lint --fix && prettier . --write",
    "browsers": "browserslist && browserslist --coverage",
    "mine": "npm run mine:data && npm run mine:images",
    "mine:data": "node scripts/mine-card-data.js",
    "mine:images": "node scripts/mine-card-images.js",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/heimruf-deck-builder/stats.json",
    "prepare": "husky"
  },
  "dependencies": {
    "@angular/animations": "^19.2.4",
    "@angular/cdk": "^19.2.7",
    "@angular/common": "^19.2.4",
    "@angular/compiler": "^19.2.4",
    "@angular/core": "^19.2.4",
    "@angular/fire": "^19.0.0",
    "@angular/forms": "^19.2.4",
    "@angular/material": "^19.2.7",
    "@angular/platform-browser": "^19.2.4",
    "@angular/platform-browser-dynamic": "^19.2.4",
    "@angular/router": "^19.2.4",
    "@ngrx/effects": "^19.0.1",
    "@ngrx/entity": "^19.0.1",
    "@ngrx/operators": "^19.0.1",
    "@ngrx/router-store": "^19.0.1",
    "@ngrx/store": "^19.0.1",
    "@ngrx/store-devtools": "^19.0.1",
    "bootstrap": "~5.3.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^19.2.5",
    "@angular/cli": "^19.2.5",
    "@angular/compiler-cli": "^19.2.4",
    "@angular/localize": "^19.2.4",
    "@ngrx/eslint-plugin": "^19.0.1",
    "@ngrx/schematics": "^19.0.1",
    "@types/node": "^22.13.14",
    "angular-eslint": "^19.3.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-regexp": "^2.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.0.11",
    "node-fetch": "^3.3.2",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.1",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.28.0",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
