{
  "name": "heimruf-deck-builder",
  "version": "1.7.10",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "deploy": "firebase deploy",
    "lint": "ng lint",
    "format": "prettier . --check",
    "fix": "ng lint --fix && prettier . --write",
    "browsers": "browserslist --coverage && browserslist",
    "browsers:update": "browserslist --update-db",
    "mine": "npm run mine:data&&npm run mine:images",
    "mine:data": "node --no-warnings=ExperimentalWarning --loader ts-node/esm tools/mine-card-data.ts",
    "mine:images": "node --no-warnings=ExperimentalWarning --loader ts-node/esm tools/mine-card-images.ts",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/heimruf-deck-builder/stats.json",
    "prepare": "husky"
  },
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/cdk": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/fire": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/material": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@ngrx/effects": "^17.0.0",
    "@ngrx/entity": "^17.0.0",
    "@ngrx/router-store": "^17.0.0",
    "@ngrx/store": "^17.0.0",
    "@ngrx/store-devtools": "^17.0.0",
    "bootstrap": "~5.3.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.0",
    "@angular-eslint/builder": "^18.0.0",
    "@angular-eslint/eslint-plugin": "^18.0.0",
    "@angular-eslint/eslint-plugin-template": "^18.0.0",
    "@angular-eslint/schematics": "^18.0.0",
    "@angular-eslint/template-parser": "^18.0.0",
    "@angular/cli": "^18.0.0",
    "@angular/compiler-cli": "^18.0.0",
    "@ngrx/eslint-plugin": "^17.0.0",
    "@ngrx/schematics": "^17.0.0",
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.9",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-regexp": "^2.1.1",
    "eslint-plugin-rxjs": "^5.0.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.1.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.4.5",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
